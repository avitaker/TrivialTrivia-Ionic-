    <icon src="resources/android/icon/Quiz-Games.png" density="xhdpi"/>
    <icon src="resources/android/icon/Quiz-Games.png" density="xxhdpi"/>
    <icon src="resources/android/icon/Quiz-Games.png" density="xxxhdpi"/>
